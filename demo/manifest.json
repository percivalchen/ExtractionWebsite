 {
  "manifest_version": 2,
  "name": "ExtrAction",
  "version": "0.7.5",
  "permissions": ["activeTab", "https://us-central1-sigma-smile-251401.cloudfunctions.net/classifier/*"],
  "description": "The goal is to be able to extract tasks and summarize them.",
  "content_security_policy": "script-src 'self' https://apis.google.com/js/api.js; object-src 'self'",
  "icons": {"16": "img/logo_v1_low_inverted.png",
            "48": "img/logo_v1_low_inverted.png",
            "128": "img/logo_v1_low_inverted.png" },
  "browser_action": {
   "default_icon": "img/logo_v1_low_inverted.png",
   "default_title": "Working model of the baseline extension",
   "default_popup": "index.html"
  }
}

